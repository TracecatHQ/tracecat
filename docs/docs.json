{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Tracecat | Documentation",
  "colors": {
    "primary": "#6F9954",
    "light": "#C1DEAF",
    "dark": "#111011"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart/install",
              "quickstart/admin",
              "quickstart/tutorial",
              "quickstart/core-actions",
              "quickstart/control-flow",
              "quickstart/expressions",
              "quickstart/secrets"
            ]
          },
          {
            "group": "Tutorials",
            "pages": [
              "tutorials/workflow-triggers",
              "tutorials/child-workflows",
              "tutorials/data-transforms",
              "tutorials/explode-implode",
              "tutorials/wait-retry-until",
              "tutorials/custom-integrations",
              "tutorials/alert-on-failure"
            ]
          },
          {
            "group": "Self Host",
            "pages": [
              "self-hosting/overview",
              {
                "group": "Deployments",
                "pages": [
                  "self-hosting/deployment-options/docker-compose",
                  "self-hosting/deployment-options/aws-ecs"
                ]
              },
              {
                "group": "Authentication",
                "pages": [
                  "self-hosting/authentication/overview",
                  "self-hosting/authentication/basic",
                  "self-hosting/authentication/google-oauth",
                  "self-hosting/authentication/saml-sso"
                ]
              },
              "self-hosting/updating"
            ]
          }
        ]
      },
      {
        "tab": "Integrations",
        "groups": [
          {
            "group": "Integrations",
            "pages": [
              "integrations/overview",
              "integrations/action-templates",
              "integrations/python-udfs",
              "integrations/development",
              "integrations/openapi-converter"
            ]
          }
        ]
      },
      {
        "tab": "Cheatsheets",
        "pages": [
          "cheatsheets/functions",
          "cheatsheets/integrations",
          "cheatsheets/yaml",
          "cheatsheets/jsonpath"
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "REST API",
            "pages": [
              "api-reference/overview"
            ]
          },
          {
            "group": "Endpoints",
            "openapi": "/openapi.yml"
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://tracecat.com",
          "icon": "globe"
        },
        {
          "anchor": "Get Live Help",
          "href": "https://cal.com/team/tracecat/onboarding",
          "icon": "headphones"
        },
        {
          "anchor": "Join Discord",
          "href": "https://discord.gg/n3GF4qxFU8",
          "icon": "discord"
        }
      ]
    }
  },
  "redirects": [
    {
      "source": "/self-hosting/introduction",
      "destination": "/self-hosting/overview"
    },
    {
      "source": "/self-hosting/authentication/introduction",
      "destination": "/self-hosting/authentication/overview"
    },
    {
      "source": "/tutorials/1-quickstart",
      "destination": "/quickstart/tutorial"
    }
  ],
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "appearance": {
    "default": "light"
  },
  "background": {
    "color": {
      "light": "#FFFFFF",
      "dark": "#111011"
    }
  },
  "navbar": {
    "links": [],
    "primary": {
      "type": "github",
      "href": "https://github.com/TracecatHQ/tracecat"
    }
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/n3GF4qxFU8",
      "github": "https://github.com/TracecatHQ/tracecat"
    }
  }
}
